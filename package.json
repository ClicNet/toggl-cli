{
  "name": "toggl-cli",
  "version": "0.0.2",
  "description": "Manage your Toggl.com time entries from the familiarity of the nearby CLI.",
  "bin": {
    "toggl": "./run.sh"
  },
  "scripts": {
    "compile": "babel src -qd compiled/src && babel test -qd compiled/test",
    "clean": "rm -rf compiled/",
    "start": "./run.sh",
    "test": "./test.sh"
  },
  "keywords": [
    "toggl",
    "cli",
    "utility",
    "tool",
    "time",
    "tracking"
  ],
  "author": "Damian Mee <mee.damian@gmail.com> (https://meedamian.com)",
  "license": "MIT",
  "engines": {
    "node": ">=4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "xo": "^0.12.1"
  },
  "dependencies": {
    "chalk": "^1.1.1",
    "elegant-spinner": "^1.0.1",
    "fs-extended": "^0.2.1",
    "log-update": "^1.0.2",
    "mee": "^0.9.0",
    "minimist": "^1.2.0",
    "request": "^2.67.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "mocha",
      "es6"
    ],
    "ignores": [
      "compiled/**"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
